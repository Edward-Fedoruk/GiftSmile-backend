{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"airbnb-base"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"linebreak-style": "off",
		"semi": "off",
		"prefer-promise-reject-errors": "off",
		"no-unused-expressions": "off"
	}
}